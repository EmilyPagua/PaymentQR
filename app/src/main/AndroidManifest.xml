<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.paymentqr" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_INTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/oko_logo_blanco"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainTabActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait"
            android:theme="@android:style/Theme.NoTitleBar" >
        </activity>
        <activity
            android:name=".MainMenu"
            android:label="@string/title_activity_main_menu" >
        </activity>
        <activity
            android:name=".CreatePayment"
            android:label="Crear Pago"
            >
        </activity>
        <activity
            android:name=".createQR.GenerateCodeQR"
            android:label="@string/title_activity_generate_code_qr" >
        </activity>
        <activity
            android:name=".scanQR.ScannerActivity"
            android:label="@string/title_activity_scanner"
            >
        </activity>
        <activity
            android:name=".TransactionDetail"
            android:label="@string/title_activity_transaction_detail" >
        </activity>
        <activity
            android:name=".webService.MainActivity2"
            android:label="@string/title_activity_main_activity2" >
        </activity>
    </application>

</manifest>
